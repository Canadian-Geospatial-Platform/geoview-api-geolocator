{
    "url": "https://geogratis.gc.ca/services/geolocation/_PARAM1_/locate",
    "urlParams": {
        "param1": "lang"
    },
    "staticParams": {
        "expand": "component"
    },
    "urlCodeTables": {},
    "lookup": {
        "in": {
            "q": "q"
        },
        "out": {
            "structure" : {
                "type": "list"
                },
            "data": {
                "name": {
                    "field": "component.name",
                    "lookup": {
                        "type": "type",
                        "field": "name"
                    }
                },
                "lat": {
                    "field":"geometry.coordinates",
                    "lookup": {
                        "type": "array",
                        "field":"1"
                    }
                },
                "lng": {
                    "field": "geometry.coordinates",
                    "lookup": {
                        "type": "array",
                        "field":"0"
                    }
                },
                "bbox": {
                    "field": "bbox",
                    "lookup": ""
                },
                "province": {
                    "field": "component.province",
                    "lookup": {
                        "type": "table",
                        "field": "description"
                    }
                },
                "tag": [
                    {
                        "field": "component.location",
                        "lookup": ""
                    },
                    {
                        "field": "component.generic",
                        "lookup": {
                            "type": "type",
                            "field": "tag"
                        }
                    }
                ]
            }
        }
    }
}
